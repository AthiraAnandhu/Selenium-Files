package CoreJavaPart3;
import java.util.ArrayList;

public class ArrayListDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ArrayList<String> a = new ArrayList<String>();
		a.add("Athira");
		a.add("Anandhu");
		System.out.println(a);
		
		a.add(0, "Student");

	}

}
